{
	"parser": "babel-eslint",
	"settings": {
		"ecmascript": 6,
		"jsx": true
	},
	"env": {
		"browser": true
	},
	"plugins": [
		"react"
	],
	"extends": "airbnb",
	"rules": {
		"strict": 2,
		"semi": 2,
		"no-unused-vars": 2,
		"camelcase": 2,
		"indent": [2, "tab"],
		"react/jsx-indent": [2, "tab"],
		"react/prefer-stateless-function": 0
	}
}
